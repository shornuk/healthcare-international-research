{% extends "_layouts/base" %}

{% set meta = {
	eyebrow: 'Legal',
	title: entry.title,
	text: entry.overview
} %}

{% block content %}

	<div class="mx-auto max-w-7xl px-6 lg:px-8 prose prose-slate dark:prose-invert">
		{% if entry.text ?? false %}
			<div class="max-w-3xl">
				{{ entry.text }}
			</div>
		{% endif %}
	</div>

{% endblock %}